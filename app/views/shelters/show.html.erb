<nav>
  <%= link_to 'Shelters Index', '/shelters' %>
  <%= link_to 'Pets Index', '/pets' %>
</nav>
<header>
  <h1><%= @shelter.name %></h1>
</header>

<body>
  <p>Address: <%= @shelter.address %></p>
  <p>City: <%= @shelter.city %></p>
  <p>State: <%= @shelter.state %></p>
  <p>Zip: <%= @shelter.zip %></p>

  <%= link_to 'Pets', "/shelters/#{ @shelter.id }/pets" %>
  <%= link_to 'Update Shelter', "/shelters/#{ @shelter.id }/edit" %>

  <%= link_to "Delete Shelter", delete_shelter_path(@shelter.id), method: :delete %>
</body>
